{
  "name": "hikvision.js",
  "version": "1.0.1",
  "description": "Cliente JS/TS para ISAPI (Hikvision) com autenticação de sessão e Digest",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "ts-node src/test.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/example.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node dist/test.js"
  },
  "keywords": [
    "hikvision",
    "isapi",
    "camera",
    "nvr",
    "dvr",
    "security",
    "surveillance",
    "typescript",
    "javascript"
  ],
  "author": "Gabriel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielpastori1/hikvision.js.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielpasto/hikvision.js/issues"
  },
  "homepage": "https://github.com/gabrielpasto/hikvision.js#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "fast-xml-parser": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
